      "import": "./dist/location/index.js",
      "types": "./dist/location/index.d.ts"
    },
    "./motion": {
      "import": "./dist/motion/index.js",
      "types": "./dist/motion/index.d.ts"
    },
    "./timing": {
      "import": "./dist/timing/index.js",
      "types": "./dist/timing/index.d.ts"
    },
