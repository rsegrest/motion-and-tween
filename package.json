      "location": [
        "./dist/location/index.d.ts"
      ],
      "timing": [
        "./dist/timing/index.d.ts"
      ],
      "tween": [
        "./dist/tween/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "keywords": [
    "motion",
    "tween",
    "motion-and-tween",
    "animation",
    "animation-library",
    "p5js",
    "p5-library"
